{
  "name": "@afetcan/service-common",
  "private": false,
  "type": "module",
  "version": "0.0.4",
  "description": "Service Common for afetcan.com",
  "author": "Mehmet - productdevbook <hi@productdevbook.com>",
  "license": "MIT",
  "homepage": "https://github.com/afetcan/#readme",
  "packageManager": "pnpm@7.26.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afetcan/storage.git"
  },
  "bugs": {
    "url": "https://github.com/afetcan/storage/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=v18.10.0",
    "pnpm": ">=7.26.3"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist && rimraf node_modules",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint --ext .ts,.tsx,.js,.jsx . --fix",
    "release": "bumpp --commit --push --tag && pnpm publish"
  },
  "peerDependencies": {
    "@sentry/node": "^7.73.0",
    "@sentry/tracing": "^7.73.0",
    "@sentry/utils": "^7.73.0"
