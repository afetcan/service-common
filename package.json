    "prom-client": "^14.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.43.1",
    "@faker-js/faker": "^7.6.0",
    "@sentry/node": "^7.73.0",
    "@sentry/tracing": "^7.73.0",
    "@sentry/types": "^7.73.0",
    "@sentry/utils": "^7.73.0",
    "bumpp": "^9.2.0",
    "consola": "^2.15.3",
    "eslint": "^8.50.0",
    "tslib": "^2.6.2",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vite": "^4.4.11",
    "vitest": "^0.34.6"
